{
  "manifest_version": 2,
  "name": "Freezy",
  "version": "0.1",

  "description": "Prevent [some] mouse events to be captured",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_scripts/freezy.js"]
    }
  ],
  "options_ui": {
    "page": "ui/options.html",
    "browser_style": true
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ]
}
